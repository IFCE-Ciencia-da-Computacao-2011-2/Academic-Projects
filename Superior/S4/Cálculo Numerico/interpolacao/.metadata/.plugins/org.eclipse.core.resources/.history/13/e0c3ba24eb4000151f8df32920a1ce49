package interpolacao;

import java.util.ArrayList;
import java.util.List;

public class Raizes {

	private List<Coordenada> raizes;

	public Raizes() {
		this.raizes = new ArrayList<Coordenada>();
	}

	public Raizes add(double fDeX, double x) {
		this.raizes.add(new Coordenada(x, fDeX));
		return this;
	}
}
