package interpolacao;

import java.util.ArrayList;
import java.util.List;

public class Raizes {

	private List<Coordenada<Double, Double>> raizes;

	public Raizes() {
		this.raizes = new ArrayList<Coordenada<Double, Double>>();
	}

	public Raizes add(double fDeX, double x) {
		this.raizes.add(new Coordenada<Double, Double>(fDeX, x));
		return this;
	}
}
